{
	"expo": {
		"name": "klover",
		"slug": "klover",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"scheme": "com.anonymous.klover",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.anonymous.klover"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package":"com.anonymous.klover"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/splash-icon.png",
					"resizeMode": "cover",
					"backgroundColor": "#ffffff",
					"enableFullScreenImage_legacy": true
				}
			],
			[
				"expo-font",
				{
					"fonts": ["./assets/fonts/Poppins-Bold.ttf"]
				}
			],
      "@xmartlabs/react-native-line"
		]
	}
}
